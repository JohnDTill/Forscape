alg rk4(f, y⁜_⏴0⏵, t⁜_⏴0⏵, t⁜_⏴f⏵, N){
    h = ⁜f⏴t⁜_⏴f⏵ - t⁜_⏴0⏵⏵⏴N - 1⏵
    Y ← 0⁜_⏴len(y⁜_⏴0⏵)×N⏵
    Y⁜_⏴:,0⏵ ← y⁜_⏴0⏵
    for(i ← 0; i < N-1; i ← i + 1){
        t⁜_⏴n⏵ = t⁜_⏴0⏵ + i*h
        y⁜_⏴n⏵ = Y⁜_⏴:,i⏵
        k⁜_⏴1⏵ = f(t⁜_⏴n⏵, y⁜_⏴n⏵) 
        k⁜_⏴2⏵ = f(t⁜_⏴n⏵ + ⁜f⏴h⏵⏴2⏵ , y⁜_⏴n⏵ + h⁜f⏴k⁜_⏴1⏵⏵⏴2⏵)
        k⁜_⏴3⏵ = f(t⁜_⏴n⏵ + ⁜f⏴h⏵⏴2⏵ , y⁜_⏴n⏵ + h⁜f⏴k⁜_⏴2⏵⏵⏴2⏵)
        k⁜_⏴4⏵ = f(t⁜_⏴n⏵ + h , y⁜_⏴n⏵ + h k⁜_⏴3⏵)
        Y⁜_⏴:,i+1⏵ ← y⁜_⏴n⏵ + ⁜f⏴1⏵⏴6⏵ h (k⁜_⏴1⏵ + 2k⁜_⏴2⏵ + 2k⁜_⏴3⏵ + k⁜_⏴4⏵)
    }
    
    return Y
}

f = (t, y) ↦ 1
print(rk4(f, 0, 0, 1, 11))